<script defer src="/themes/core/static/js/vendor.bundle.min.js?d=3ad721ae"></script>
<script defer src="/themes/core/static/js/core.min.js?d=3ad721ae"></script>
<script defer src="/themes/core/static/js/helpers.min.js?d=3ad721ae"></script>
<script defer src="/themes/core/static/js/pages/main.min.js?d=3ad721ae"></script>
<script>
		// Apparently countdown needs to be here for some reason??
		// instead of in the home page. idk probably for browser compatibility
		// originally this was in the theme footer in ctfd
		// trust kuilin
		(function () {
			// Update the count down every 1 second
			var interval;
			var update = function () {
				var startTime = 1659139200000;
				var endTime = 1659312000000;
				var currentTime = new Date().getTime();

				var beforeStart = currentTime < startTime;
				var countDownDate = new Date(beforeStart ? startTime : endTime).getTime();

				// Only run on the main page
				if (!document.getElementById("demo")) return;

				// Get today's date and time
				var now = new Date().getTime();

				// Find the distance between now and the count down date
				var distance = countDownDate - now;

				// Time calculations for days, hours, minutes and seconds
				var days = Math.floor(distance / (1000 * 60 * 60 * 24));
				var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
				var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
				var seconds = Math.floor((distance % (1000 * 60)) / 1000);

				// Display the result in the element with id="demo"
				var verb = beforeStart ? "starts" : "ends";
				document.getElementById("demo").innerHTML = "UIUCTF " + verb + " in " + days + "d " + hours + "h "
					+ minutes + "m " + seconds + "s ";

				// If the count down is finished, write some text
				if (distance < 0 && !beforeStart) {
					clearInterval(interval);
					document.getElementById("demo").innerHTML = "UIUCTF has ended!";
				}
			};
			interval = setInterval(update, 1000);
			update();
		})();
	</script>
</body>
